{
	"coc.preferences.formatOnSaveFiletypes":["javascript","json","vim","python"],
  "prettier.singleQuote": true,
  "prettier.eslintIntegration": true,
  "eslint.autoFixOnSave": false,
  "eslint.packageManager": "npm",
  "python.jediEnabled": false,
  "python.formatting.provider":"autopep8",
  "python.formatting.autopep8Args": ["--max-line-length=90"],
  "python.formatting.blackArgs": ["--max-line-length=90"],
  "python.linting.enabled": true,
  "python.linting.lintOnSave": true,
  "python.linting.flake8Args": ["--max-line-length=90"],
  "python.linting.pylintArgs": ["--disable=C0114, C0115, E1101, C0116","--max-line-length=90"],
  "python.linting.pep8Args":["--max-line-length=90"],
  "suggest.noselect": false,
  "explorer.width": 30,
  "explorer.icon.enableNerdfont": true,
  "explorer.previewAction.onHover": false,
  "explorer.keyMappings": {"<cr>": ["expandable?", "expand", "open"],"v": "open:vsplit"}
}
