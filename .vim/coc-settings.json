{
  "coc.preferences.formatOnSaveFiletypes": [ "javascript","python" ],
  "prettier.singleQuote": true,
  "prettier.eslintIntegration": true,
  "eslint.autoFixOnSave": true,
  "python.formatting.provider":"yapf",
  "python.formatting.autopep8Args": ["--max-line-length=90"],
  "python.formatting.blackArgs": ["--max-line-length=90"],
  "python.linting.enabled": true,
  "python.linting.lintOnSave": true,
  "python.linting.pylintEnabled": false,
  "python.linting.pylintArgs": ["--disable=C0114, C0115, E1101, C0116","--max-line-length=90"],
  "python.linting.pep8Enabled": false,
  "python.linting.pep8Args":["--max-line-length=90"],
  "python.linting.flake8Enabled":true,
  "python.linting.flake8Args": ["--max-line-length=90"], 
  "explorer.icon.enableNerdfont": true
}
